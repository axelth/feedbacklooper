<ul>
<% compositions.each do |c| %>
  <li>課題<%=c.assignment.title%>の<a href="<%=url('/student/respond/')%><%=c.id%>">フィードバックに回答する</a>
    <% errors = feedback.select {|e| e.composition == c} %>
  <% if errors && errors.length > 0 %>
    <table>
      <% errors.each do |e| %>
	<tr><td><%=e.styled_line_with_breaks(10)%></td><td><%= e.string %></td><td><%= e.correction %></td></tr>
      <% end %>
    </table>
  <% end %>
  </li>
<% end %>
</ul>
